    "express": "^4.18.2",
    "framer-motion": "^12.23.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.1",
    "css-loader": "^7.1.2",
