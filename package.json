{
  "name": "fullstack-react-express-sql",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev:server": "nodemon src/server/index.js",
    "dev:client": "webpack serve --config webpack.config.js --mode development",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
